{
    "listOfRulesBasedOnTicketValues":[
        {
            "settingName":"src/scan/colorMode",
            "rules":[
                {
                    "if":[
                        {
                            "settingName":"src/scan/mediaType",
                            "withValueIn": [
                                {"unionValue":{"value":"TRANSLUCENT_PAPER"},"unionValue_type":"OriginalMediaType"},
                                {"unionValue":{"value":"WHITE_PAPER"},"unionValue_type":"OriginalMediaType"}
                            ]                                        
                        }
                    ],
                    "then":{
                        "supportedValues": [
                            {"unionValue":{"value":"COLOR"},"unionValue_type":"ColorMode"},
                            {"unionValue":{"value":"GRAYSCALE"},"unionValue_type":"ColorMode"}
                        ]
                    },
                    "rulePolicy":"CONTINUE"
                },
                {
                    "if":[
                        {
                            "settingName":"src/scan/mediaType",
                            "withValueIn": [
                                {"unionValue":{"value":"TRANSLUCENT_PAPER"},"unionValue_type":"OriginalMediaType"},
                                {"unionValue":{"value":"WHITE_PAPER"},"unionValue_type":"OriginalMediaType"}
                            ]                                        
                        }
                    ],
                    "then":{
                        "supportedValues": [
                            {"unionValue":{"value":"TRANSLUCENT_PAPER"},"unionValue_type":"OriginalMediaType"}
                        ]
                    }
                }
            ]
        }
    ]
}