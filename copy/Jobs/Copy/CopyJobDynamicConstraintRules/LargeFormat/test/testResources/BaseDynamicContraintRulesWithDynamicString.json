{
    "listOfRulesBasedOnTicketValues":[
        {
            "settingName":"src/scan/contentType",
            "rules":[
                {                    
                    "if":[
                        {
                            "settingName":"src/scan/mediaType",
                            "withValueIn": [
                                {"unionValue":{"value":"TRANSLUCENT_PAPER"},"unionValue_type":"OriginalMediaType"},
                                {"unionValue":{"value":"BLUEPRINTS"},"unionValue_type":"OriginalMediaType"}
                            ]                                        
                        }
                    ],
                    "then":{
                        "supportedValues": [
                            {"unionValue":{"value":"LINE_DRAWING"},"unionValue_type":"OriginalContentType"}
                        ],                                    
                        "disabled":true
                    }
                },
                {
                    "if":[
                        {
                            "settingName":"dest/print/copies",
                            "withValueIn": [
                                {"unionValue":{"value":1},"unionValue_type":"INT32"}
                            ]
                        }
                    ],
                    "then":{
                        "supportedValues": [
                            {"unionValue":{"value":"LINE_DRAWING"},"unionValue_type":"OriginalContentType"},
                            {"unionValue":{"value":"IMAGE"},"unionValue_type":"OriginalContentType"}
                        ]
                    }
                }
            ]
        },
        {
            "settingName":"src/scan/mediaType",
            "rules":[
                {
                    "if":[
                        {
                            "settingName":"dest/print/copies",
                            "withValueIn": [
                                {"unionValue":{"value":4},"unionValue_type":"INT32"}
                            ]
                        },
                        {
                            "settingName":"src/scan/contentType",
                            "withValueIn": [
                                {"unionValue":{"value":"MIXED"},"unionValue_type":"OriginalContentType"}
                            ]                                        
                        }
                    ],
                    "then":{
                        "supportedValues": [
                            {"unionValue":{"value":"DARK_BLUEPRINTS"},"unionValue_type":"OriginalMediaType"}
                        ],                                    
                        "disabled":true
                    }
                },
                {
                    "ifOperation":"OR",
                    "if":[
                        {
                            "settingName":"dest/print/copies",
                            "withValueIn": [
                                {"unionValue":{"value":4},"unionValue_type":"INT32"}
                            ]
                        },
                        {
                            "settingName":"src/scan/contentType",
                            "withValueIn": [
                                {"unionValue":{"value":"MIXED"},"unionValue_type":"OriginalContentType"}
                            ]                                        
                        }
                    ],
                    "then":{
                        "supportedValues": [
                            {"unionValue":{"value":"DARK_BLUEPRINTS"},"unionValue_type":"OriginalMediaType"}
                        ],                                    
                        "disabled":true
                    }
                }
            ]
        },
        {
            "settingName":"pipelineOptions/imageModifications/backgroundColorRemovalLevel",
            "rules":[
                {
                    "ifOperation":"NAND",
                    "if":[
                        {
                            "settingName":"src/scan/mediaType",
                            "withValueIn": [
                                {"unionValue":{"value":"WHITE_PAPER"},"unionValue_type":"OriginalMediaType"},
                                {"unionValue":{"value":"PHOTO_PAPER"},"unionValue_type":"OriginalMediaType"},
                                {"unionValue":{"value":"OLD_RECYCLED_PAPER"},"unionValue_type":"OriginalMediaType"},
                                {"unionValue":{"value":"BLUEPRINTS"},"unionValue_type":"OriginalMediaType"}
                            ]                                        
                        }
                    ],
                    "then":{
                        "supportedValues": [
                            {"unionValue": {"value":0}, "unionValue_type":"INT32"}
                        ],                                    
                        "disabled":true
                    }
                }
            ]
        },
        {
            "settingName":"dest/print/copies",
            "rules":[
                {
                    "ifOperation":"NOR",
                    "if":[
                        {
                            "settingName":"pipelineOptions/imageModifications/backgroundColorRemovalLevel",
                            "withValueIn":[
                                {"unionValue":{"value":4},"unionValue_type":"INT32"}
                            ]
                        }
                    ],
                    "then":{
                        "supportedValues": [
                            {   
                                "unionValue":
                                {
                                    "min":0,
                                    "max":50,
                                    "step":0
                                },
                                "unionValue_type":"RangeValueInt"
                            }
                        ],                                    
                        "disabled":true
                    }
                }
            ]
        },
        {
            "settingName":"pipelineOptions/imageModifications/outputCanvasOrientation",
            "rules":[
                {
                    "if":[
                        {
                            "settingName":"src/scan/mediaType",
                            "withValueIn": [
                                {"unionValue":{"value":"OLD_RECYCLED_PAPER"},"unionValue_type":"OriginalMediaType"}
                            ]
                        }
                    ],
                    "then":{
                        "supportedValues": [
                            {"unionValue":{"value":"PORTRAIT"},"unionValue_type":"ContentOrientation"}
                        ],
                        "disabled":true
                    }
                }
            ]
        }
    ],
    "listOfDynamicForceSets": [],
    "vectorMapStringIdForSettings":[
        {
            "settingName":"dest/print/copies",
            "vectorStringId":["StringIds.cCopies"]
        },
        {
            "settingName":"src/scan/mediaType",
            "vectorStringId":["StringIds.cOriginalPaperType"],
            "stringIdsForValuesOfSetting":[
                {"settingNameValue":"_undefined_","stringId":"StringIds.cUndefined"},
                {"settingNameValue":"blueprints","stringId":"StringIds.cBlueprint"},
                {"settingNameValue":"darkBlueprints","stringId":"StringIds.cAmmoniaOldBlueprint"},
                {"settingNameValue":"oldRecycledPaper","stringId":"StringIds.cPaperTypeOldRecycled"},
                {"settingNameValue":"photoPaper","stringId":"StringIds.cPhoto"},
                {"settingNameValue":"translucentPaper","stringId":"StringIds.cPaperTypeTranslucent"},
                {"settingNameValue":"whitePaper","stringId":"StringIds.cColorWhite"}
            ]
        },
        {
            "settingName":"src/scan/contentType",
            "vectorStringId":["StringIds.cContentType"],
            "stringIdsForValuesOfSetting":[
                {"settingNameValue":"mixed","stringId":"StringIds.cMixed"},
                {"settingNameValue":"image","stringId":"StringIds.cImage"},
                {"settingNameValue":"lineDrawing","stringId":"StringIds.cLines"}
            ]
        },
        {
            "settingName":"pipelineOptions/imageModifications/backgroundColorRemovalLevel",
            "vectorStringId":["StringIds.cDetailedBackgroundRemoval"]
        },
        {
            "settingName":"pipelineOptions/imageModifications/outputCanvasOrientation",
            "vectorStringId":["StringIds.cOutputSize","StringIds.cOrientation"],
            "stringIdsForValuesOfSetting":[
                {"settingNameValue":"_undefined_","stringId":"StringIds.cUndefined"},
                {"settingNameValue":"landscape","stringId":"StringIds.cLandscape"},
                {"settingNameValue":"portrait","stringId":"StringIds.cPortrait"}
            ]
        },
        {
            "settingName":"dest/print/foldingStyleId",
            "vectorStringId":["StringIds.cFoldingStyle"]
        },
        {
            "settingName":"dest/print/mediaDestination",
            "vectorStringId":["StringIds.cOutputDestination"],
            "stringIdsForValuesOfSetting":[
                {"settingNameValue":"_undefined_","stringId":"StringIds.cUndefined"},
                {"settingNameValue":"autoselect","stringId":"StringIds.cAuto"},
                {"settingNameValue":"default_","stringId":"StringIds.cDefault"},
                {"settingNameValue":"stacker","stringId":"StringIds.cStacker"},
                {"settingNameValue":"stacker_dash_1","stringId":"StringIds.cStacker"},
                {"settingNameValue":"folder","stringId":"StringIds.cFolderOutput"},
                {"settingNameValue":"folder_dash_1","stringId":"StringIds.cFolderOutput"}
            ]
        }
    ]
}
