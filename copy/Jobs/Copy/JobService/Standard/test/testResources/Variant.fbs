
include "Uuid.fbs";

namespace dune.print.engine;

///< @union FieldValueType
///< Union of all possible value types
union FieldValueType {FLOAT, INT32, BOOL, TEXT, INT16, UUID, ARRAY, FLOAT_ARRAY, BYTE_ARRAY, INT8, UINT8, UINT16, UINT32, INT64, UINT64}

table TEXT {
  value : string (required);
}
table FLOAT {
  value : float = 0.0;
}
table INT32 {
  value : int32 = -1;
}
table INT16 {
  value : int16 = -1;
}
table BOOL {
  value : bool = false;
}
table UUID {
  value : dune.framework.core.uuid.fbs.Uuid (native_inline);
}

table ARRAY {
  value : [int32];
}

table FLOAT_ARRAY {
  value : [float];
}

table BYTE_ARRAY {
  value : [ubyte];
}

table INT8 {
  value : int8 = -1;
}

table UINT8 {
  value : uint8 = 0;
}

table UINT16 {
  value : uint16 = 0;
}

table UINT32 {
  value : uint32 = 0;
}

table INT64 {
  value : int64 = -1;
}

table UINT64 {
  value : uint64 = 0;
}

table VariantValue
{
  value: FieldValueType;
}

root_type VariantValue;
