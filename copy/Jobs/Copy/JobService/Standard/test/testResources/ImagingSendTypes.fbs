namespace dune.imaging.types;

enum CompressionType : byte
{
    DEFAULT = 0,
    AUTO,
    RAW,
    PNM,
    JPEG,
    OJPEG,
    ZJPEG,
    JPEG2000,
    LZW,
    PNG,
    MH,
    MR,
    MMR,
    G3,
    G4,
    JBIG,
    JBIG2,
    MRC,
    URF,
    JPEG_FAX,
    FLATE,
    JPGD,
    ZJPGD,
    MRC_SEG,
    OCR_CSV,
    OCR_PDF,
    OCR_PDFA,
    OCR_TEXT,
    OCR_UNICODE,
    OCR_HTML,
    OCR_RTF,
    JPEG_STRIP,
    PCL5_PACKBITS,
    PCL5_UNCOMPRESSED,
}

enum RotationMethod : byte
{
    /** Use the default (same as JPEG_OR_RASTER) */
    DEFAULT = 0,
    /** Use lossless JPEG if possible, otherwise raster */
    JPEG_OR_RASTER,
    /** Use Raster rotation always */
    RASTER
}

enum DocumentType : byte
{
    DEFAULT = 0,
    TEXT,
    PHOTO,
    MIXED
}

enum FileSize : byte
{
    DEFAULT = 0,
    SMALL,
    STANDARD,
    LARGE,

    // These are used for TBMRC layer generation only, for low res (200 DPI) TBMRC.
    // See TbmrcPageSegmenter for more details about this usage.
    TBMRC_LOWRES_SMALL,
    TBMRC_LOWRES_STANDARD,
    TBMRC_LOWRES_LARGE,
    TBMRC_LOWRES_SMALLER,
    TBMRC_LOWRES_LARGER,

    SMALLER,
    LARGER
}

enum PdfVersion : byte
{
    DEFAULT = 0,
    PDF_14,
    PDF_17,
}

enum ReturnType : int
{
    FAIL = 0,
    SUCCESS,
    CANCELED,
}
